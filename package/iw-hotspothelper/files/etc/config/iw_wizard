config general service
  option mesh_network yes
  option captive_portal yes

config wireless wifi
  option conf_file wireless
  option conf_name interface
  option conf_value wlan
	option device   'wl0'
	option network	'wlan'
	option mode	'adhoc'
	option ssid	'Internet-Wifi'

config network interface
  option interface 'wlan'
	option proto	'static'
	option ipaddr	'10.128.12.1'
	option netmask	'255.255.0.0'
	option dns	'172.16.0.5 204.225.44.3'
